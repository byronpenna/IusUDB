@section estilosPersonalizados{
    <link rel="stylesheet" href="~/Content/ius_theme/views/Conocenos/style.css" />
    <link rel="stylesheet" href="~/Content/ius_theme/views/Conocenos/media.css" />
}
@section scriptsPersonalizados{
    <!-- Plugins -->
    
    <!-- propios -->
    <script src="~/Scripts/views/Conocenos/script.js"></script>
    <script src="~/Scripts/views/Conocenos/functions.js"></script>
}
<div class="row marginNull divMenus">
    <input type="hidden" class="txtHdSeleccionadoBuble" value="@ViewBag.seleccionado"/>
    <div class="row marginNull">
        <div class="col-lg-offset-3 col-lg-6 text-center imgCambioContenedor">
            <div class="divImgCambio" id="1"><!-- Este deberia ser 2 pero por orden tendre que ponerle 1-->
                <img class="imgCambio" src="~/Content/images/generales/ico/contacts.png" />
                <div class="divTxtCambio">@ViewData["burbuja-1"]</div>
            </div>
            <div class="divImgCambio divImgCambioPrimero" id="2"><!-- Este deberia ser 1 pero por orden tendre que ponerle 2-->
                <img class="imgCambio" src="~/Content/images/generales/ico/clock.png" />
                <div class="divTxtCambio">@ViewData["burbuja-2"]</div>
            </div>
            <div class="divImgCambio" id="3">
                <img class="imgCambio" src="~/Content/images/generales/ico/trophy.png" />
                <div class="divTxtCambio">@ViewData["burbuja-3"]</div>
            </div>
            <div class="divImgCambio" id="4">
                <img class="imgCambio" src="~/Content/images/generales/ico/document.png" />
                <div class="divTxtCambio">@ViewData["burbuja-4"]</div>
            </div>
            <div class="divImgCambio" id="5">
                <img class="imgCambio" src="~/Content/images/generales/ico/pie-chart.png" />
                <div class="divTxtCambio">@ViewData["burbuja-5"]</div>
            </div>
        </div>
    </div>
    <div class="row marginNull">
        <div class="row marginNull divContenedorInfoL">
            
        </div>
    </div>
</div>
