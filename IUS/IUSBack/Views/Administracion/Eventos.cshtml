@{
    /*
	id_usuario_creador_fk	int, -- usuario que creo el evento
	id_tipoaccesso_fk 		int,
	fecha_creacion 			datetime*/
}
<h2>Eventos</h2>
<div class="row">
    <div class="col-lg-4">
        <form id="frmAgregarEvento">
            <div class="form-group">
                <label for="txtEvento">Nombre evento</label>
                <input type="text" class="form-control txtEvento" name="txtEvento" id="txtEvento" placeholder="Evento">
            </div>
            <div class="form-group">
                <label for="txtAreaDescripcion">Descripcion:</label>
                <textarea class="form-control txtAreaDescripcion" id="txtAreaDescripcion" name="txtAreaDescripcion" placeholder="Breve explicacion sobre que consiste eventos"></textarea>
            </div>
            <div class="form-group">
                <label for="txtFechaInicio">Fecha inicio</label>
                <input type="text" class="form-control txtFechaInicio" name="txtFechaInicio" id="txtFechaInicio" />
            </div>
            <div class="form-group">
                <label for="txtFechaFin">Fecha fin</label>
                <input type="text" class="form-control txtFechaFin" name="txtFechaFin" id="txtFechaFin" />
            </div>
            <div class="form-group">
                <label for="cbTipoAcceso">Tipo de acceso </label>
                <select class="form-control cbTipoAcceso" name="cbTipoAcceso" id="cbTipoAcceso" >
                    
                </select>
            </div>
            <div class="form-group">
                <button class="btnAgregarEvento btn btn-primary">
                    Agregar evento
                </button>
            </div>
        </form>
    </div>
    <div class="col-lg-8">

    </div>
</div>
